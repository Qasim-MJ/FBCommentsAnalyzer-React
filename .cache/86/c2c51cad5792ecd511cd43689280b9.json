},"source":"app/screens/dashboard.js","original":{"line":90,"column":51}},{"generated":{"line":185,"column":14},"source":"app/screens/dashboard.js","original":{"line":90,"column":55}},{"generated":{"line":185,"column":16},"source":"app/screens/dashboard.js","original":{"line":90,"column":56}},{"generated":{"line":185,"column":31},"source":"app/screens/dashboard.js","original":{"line":90,"column":44}},{"generated":{"line":186,"column":0},"source":"app/screens/dashboard.js","original":{"line":90,"column":72}},{"generated":{"line":186,"column":10},"source":"app/screens/dashboard.js","original":{"line":90,"column":72}},{"generated":{"line":186,"column":19},"source":"app/screens/dashboard.js","original":{"line":90,"column":81}},{"name":"Comments","generated":{"line":186,"column":21},"source":"app/screens/dashboard.js","original":{"line":90,"column":83}},{"generated":{"line":187,"column":0},"source":"app/screens/dashboard.js","original":{"line":90,"column":44}},{"generated":{"line":187,"column":10},"source":"app/screens/dashboard.js","original":{"line":87,"column":44}},{"generated":{"line":187,"column":11},"source":"app/screens/dashboard.js","original":{"line":86,"column":40}},{"generated":{"line":187,"column":12},"source":"app/screens/dashboard.js","original":{"line":85,"column":36}},{"generated":{"line":187,"column":13},"source":"app/screens/dashboard.js","original":{"line":84,"column":32}},{"generated":{"line":187,"column":14},"source":"app/screens/dashboard.js","original":{"line":72,"column":28}},{"generated":{"line":187,"column":15},"source":"app/screens/dashboard.js","original":{"line":71,"column":24}},{"generated":{"line":188,"column":0},"source":"app/screens/dashboard.js","original":{"line":98,"column":21}},{"generated":{"line":188,"column":7},"source":"app/screens/dashboard.js","original":{"line":67,"column":12}},{"generated":{"line":188,"column":8},"source":"app/screens/dashboard.js","original":{"line":66,"column":8}},{"generated":{"line":189,"column":0},"source":"app/screens/dashboard.js","original":{"line":102,"column":5}},{"name":"Component","generated":{"line":193,"column":2},"source":"app/screens/dashboard.js","original":{"line":63,"column":39}}],"sources":{"app/screens/dashboard.js":"import React, {Component} from 'react'\nimport styled from 'styled-components'\nimport Pages from '../components/pages'\nimport Posts from '../components/posts'\nimport Comments from '../components/comments'\nimport { Route, Link, BrowserRouter as Router } from 'react-router-dom'\nimport Context from '../components/Context'\n\nlet Container = styled.div`\n    width : 100%;\n    padding:0;\n    margin:0;\n    display : flex;\n    justify-content:center;\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif ;\n\n`\nlet LeftDiv = styled.div`\n    width : 20%;\n    height : 100vh;\n    display : flex;\n    flex-direction : column ;\n    align-items : center ;\n    padding:10px;\n\n`\nlet DashboardTitle = styled.h1`\n    color : #4C6AB5 ;\n    font-size: 2rem;\n`\nlet UserDataDiv = styled.div`\n    width : 80%;\n    display : flex;\n    align-items : center;\n    justify-content : space-evenly;\n`\nlet UserImage = styled.img`\n    border-radius : 25px;\n`\nlet UserName = styled.p`\n    font-size : 1rem;\n    opacity : 0.8;\n    font-weight : bold;\n    margin : 0;\n`\nlet UserId = styled.p`\n    font-size : 0.7rem;\n    opacity : 0.5;\n    font-weight : bold;\n    margin : 0;\n`\n\nlet RightDiv = styled.div`\n    width : 79%;\n    min-height : 100vh;\n    border-left-style: solid;\n    border-left-color : #eaeaea;\n    border-left-width: 3px;\n`\n\nlet RouterDiv = styled.div`\n`\nexport default class Dashboard extends Component {\n    render(){\n        console.log(JSON.parse(localStorage.getItem('userFacebookData')))\n        return (\n            <Context.Consumer>\n                {\n                    (ctx)=>{\n                        let userFacebookData = JSON.parse(localStorage.getItem('userFacebookData'))\n                        return (\n                            <Container>\n                                <LeftDiv>\n                                    <DashboardTitle>Dashboard</DashboardTitle>\n                                    <UserDataDiv>\n                                        <UserImage src={userFacebookData.picture.data.url} />\n                                        <div>\n                                        <UserName>{userFacebookData.name}</UserName>\n                                        <UserId>{userFacebookData.userID}</UserId>\n                                        </div>\n                                    </UserDataDiv>\n                                <button onClick={()=>{ctx.actions.logout()}}>Log out</button>\n                                </LeftDiv>\n                                <RightDiv>\n                                    <RouterDiv>\n                                        <Router>\n                                            <div>\n                                            <Route exact path=\"/\" component={Pages} />\n                                            <Route path=\"/posts/:id\" component={Posts} />\n                                            <Route path=\"/comments/:id\" component={Comments} />\n                                            </div>\n                                        </Router>\n                                    </RouterDiv>\n                                </RightDiv>\n                                    \n                            </Container>\n                        )\n                    }\n                }\n            </Context.Consumer>\n        )\n    }\n}"},"lineCount":null}},"hash":"d1fe69c2452d27d1821f95144ebd9eb0","cacheData":{"env":{}}}