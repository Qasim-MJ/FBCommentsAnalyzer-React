"original":{"line":61,"column":15},"generated":{"line":94,"column":1}}],"sources":{"../src/Comments.jsx":"// @flow\nimport React, { forwardRef, PureComponent, type Node } from 'react';\nimport Parser from './Parser';\nimport getCurrentHref from './utils/getCurrentHref';\n\ntype Props = {\n  href?: string,\n  numPosts?: number,\n  orderBy?: string,\n  width?: number | string,\n  colorScheme?: string,\n  children?: Node,\n  mobile?: boolean,\n  handleParse: Function,\n};\n\nclass Comments extends PureComponent<Props> {\n  static defaultProps = {\n    href: undefined,\n    numPosts: undefined,\n    orderBy: undefined,\n    width: undefined,\n    colorScheme: undefined,\n    children: undefined,\n    mobile: undefined,\n  };\n\n  componentDidUpdate() {\n    const { handleParse } = this.props;\n    handleParse();\n  }\n\n  render() {\n    const {\n      colorScheme,\n      href = getCurrentHref(),\n      numPosts,\n      orderBy,\n      width,\n      children,\n      mobile,\n    } = this.props;\n\n    return (\n      <div\n        className=\"fb-comments\"\n        data-colorscheme={colorScheme}\n        data-numposts={numPosts}\n        data-href={href}\n        data-order-by={orderBy}\n        data-width={width}\n        data-skin={colorScheme}\n        data-mobile={mobile}\n      >\n        {children}\n      </div>\n    );\n  }\n}\n\nexport default forwardRef((props, ref) => (\n  <Parser>\n    {({ handleParse }) => (\n      <Comments\n        {...props}\n        handleParse={handleParse}\n        ref={ref}\n      />\n    )}\n  </Parser>\n));\n"},"lineCount":null}},"hash":"633a45ade0492005da6b77d96b342c67","cacheData":{"env":{}}}