acebook.com/v3.2/me?access_token=${userFacebookData.accessToken}`\n        fetch(url , {\n            method : 'GET'\n             })\n             .then((res)=>{\n                 return res.json()\n             }) \n             .then((data)=>{\n                 if (data.id) {\n                     this.setState({isLogedin : true})\n                 } else {\n                    localStorage.removeItem(\"userFacebookData\")\n\n                 }\n \n             })\n       }\n\n       \n    }\n\n    _checklogin(){\n        if (this.state.isLogedin) {\n            return (\n               <Dashboard />\n            )\n        } else {\n            return (\n                <Landing/>\n            )\n        }\n    }\n\n    render() {\n         return ( \n             <Context.Provider value={{\n                 state : this.state ,\n                 actions : {\n                     login : (response)=>{\n                        if (response.error) {\n                            this.setState({isLogedin : false})\n                            alert('An Error Occured')\n                        } else {\n                            localStorage.setItem(\"userFacebookData\" , JSON.stringify(response))\n                        this.setState({isLogedin : true})\n                        }\n                     },\n                     logout : ()=>{\n                        localStorage.removeItem(\"userFacebookData\")\n                        this.setState({isLogedin : false})\n                        window.open('/' , '_self')\n                     }\n                 }\n             }}>\n                {this._checklogin()}\n                \n             </Context.Provider>\n         )\n    }\n}\n\nReactDOM.render(<App />, document.getElementById('root'));"},"lineCount":null}},"hash":"f661113ee7c5e04b1cef5e86ecf97ba3","cacheData":{"env":{}}}